{
  "name": "www.trelobifteki.com",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "axios": "0.19.1",
    "prismjs": "1.19.0",
    "reset-css": "5.0.1",
    "vue": "2.6.11",
    "vue-router": "3.1.5",
    "vuex": "3.1.2",
    "webpack": "4.41.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.1.2",
    "@vue/cli-plugin-e2e-cypress": "4.1.2",
    "@vue/cli-plugin-eslint": "4.1.2",
    "@vue/cli-plugin-unit-jest": "4.1.2",
    "@vue/cli-service": "4.1.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "6.1.2",
    "fontello-webpack-plugin": "git://github.com/sypl/fontello-webpack-plugin.git#master",
    "html-loader": "0.5.5",
    "jest-junit": "10.0.0",
    "markdown-loader": "5.1.0",
    "node-sass": "4.13.0",
    "prerender-spa-plugin": "3.4.0",
    "robotstxt-webpack-plugin": "7.0.0",
    "sass-loader": "8.0.0",
    "sitemap-webpack-plugin": "0.6.0",
    "vue-analytics": "5.22.1",
    "vue-meta": "2.3.1",
    "vue-template-compiler": "2.6.11",
    "webpack-cdn-plugin": "3.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "prefer-arrow-callback": [
        "error"
      ],
      "array-bracket-spacing": 1,
      "object-shorthand": [
        "error",
        "always"
      ],
      "no-var": "error"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      },
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
